{
  "name": "task-manager",
  "version": "1.0.0",
  "description": "A task management application built with TypeScript, React, and Node.js",
  "private": true,
  "scripts": {
    "start:client": "webpack serve",
    "start:server": "nest start --watch",
    "start:dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:prod": "node dist/server/main",
    "clean": "rm -rf dist",
    "build:client": "webpack",
    "build:server": "nest build",
    "build:full": "npm run build:server && npm run build:client",
    "migrate:dev": "tsx node_modules/.bin/knex migrate:latest",
    "migrate:prod": "knex migrate:latest --knexfile=./dist/knexfile.js --env production",
    "type-check": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p client/tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CosmoS1X/new-typescript-project.git"
  },
  "author": "Konstantin Korovin <cosmo.stan.x@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CosmoS1X/new-typescript-project/issues"
  },
  "homepage": "https://github.com/CosmoS1X/new-typescript-project#readme",
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@reduxjs/toolkit": "^2.7.0",
    "bootstrap": "^5.3.5",
    "bootstrap-icons": "^1.11.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "classnames": "^2.5.1",
    "debug": "^4.4.0",
    "dotenv": "^16.5.0",
    "express-session": "^1.18.2",
    "i18next": "^25.1.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "knex": "^3.1.0",
    "objection": "^3.1.5",
    "pg": "^8.15.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.2",
    "react-i18next": "^15.5.1",
    "react-redux": "^9.2.0",
